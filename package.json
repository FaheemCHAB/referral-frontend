    "@angular/platform-browser": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@heroicons/react": "^2.1.1",
    "chart.js": "^4.4.8",
    "google-libphonenumber": "^3.2.40",
    "intl-tel-input": "^19.5.7",
    "ngx-bootstrap": "^6.2.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "^7.8.1",
    "serve": "^14.2.4",
    "tslib": "^2.5.0",
